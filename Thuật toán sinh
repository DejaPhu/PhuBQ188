#include<bits/stdc++.h>
using namespace std;

int n, a[100], ok;

void ktao(){
    for(int i = 1; i <= n; i++){
        a[i] = 0; // Khởi tạo cấu hình đầu tiên
    }
}

void sinh(){
    int i = n;
    while(i >= 1 && a[i] == 1){ // Khi chưa phải cấu hình cuối cùng
        a[i] = 0;
        --i;
    }
    if(i == 0){
        ok = 0; // Đây là cấu hình cuối cùng
    }
    else{
        a[i] = 1;
    }
}

bool check(){
    int l = 1, r = n;
    while(l <= r){
        if(a[l] != a[r]){
            return false;
        }
        l++;
        r--;
    }
    return true;
}

int main(){
    cin >> n;
    ok = 1;
    ktao();
    while(ok){
        if(check()){
            for(int i = 1; i <= n; i++){
                cout << a[i]; // In ra cấu hình hiện tại
                if(i < n) cout << " ";
            }
            cout << endl;
        }
        sinh(); // Sinh ra cấu hình tiếp theo
    }
    return 0;
}
